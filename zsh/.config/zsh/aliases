# listing
alias ls='exa'
alias la='exa -aFx'
alias ll='exa -laF --icons'

# tmux
alias t='tmux'
alias ta='tmux a'
alias tls='tmux ls'

# git
alias gs='git status'
alias ga='git add'
alias gc='git commit'
alias gd='git diff'
alias gP='git push'
alias gp='git pull'
alias gl1='git log --oneline'

# vim/nvim
alias v='nvim'

# misc
alias ed='ed -p"ed: "'

