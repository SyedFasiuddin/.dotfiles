snippet enum "Enumerate: ordered list" b
\begin{enumerate}
	\item $0
\end{enumerate}
endsnippet

snippet item "Itemize: unordered list" b
\begin{itemize}
	\item $0
\end{itemize}
endsnippet

snippet desc "Description: list with bold tilte points" b
\begin{description}
	\item[$1] $0
\end{description}
endsnippet

snippet - "Item: create item in all lists" ib
\item $0
endsnippet
